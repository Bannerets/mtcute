{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2020",
        "allowJs": true,
        "sourceMap": true,
        "inlineSources": true,
        "declaration": true,
        "esModuleInterop": true,
        "strict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "incremental": true,
        "stripInternal": true
    },
    "exclude": [
        "private",
        "node_modules"
    ]
}

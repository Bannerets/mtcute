{
    "name": "@mtcute/test",
    "private": true,
    "version": "0.1.0",
    "description": "Test utilities for mtcute",
    "author": "Alina Sireneva <alina@tei.su>",
    "license": "MIT",
    "main": "src/index.ts",
    "type": "module",
    "scripts": {

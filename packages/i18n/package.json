{
    "name": "@mtcute/i18n",
    "private": true,
    "version": "0.7.0",
    "description": "I18n for mtcute",
    "author": "Alina Sireneva <alina@tei.su>",
    "license": "MIT",
    "main": "src/index.ts",
    "type": "module",
    "scripts": {
        "build": "pnpm run -w build-package i18n"
    },
    "distOnlyFields": {
        "exports": {
            ".": {
                "import": "./esm/index.js",
                "require": "./cjs/index.js"
            },
            "./plurals/*": {
                "import": "./esm/plurals/*",
                "require": "./cjs/plurals/*"
            }
        }
    },
    "devDependencies": {
        "@mtcute/core": "workspace:^",
        "@mtcute/dispatcher": "workspace:^"
    }
}

{
    "name": "@mtcute/tl-runtime",
    "private": true,
    "version": "0.3.0",
    "description": "Runtime for TL",
    "author": "Alina Sireneva <alina@tei.su>",
    "license": "MIT",
    "type": "module",
    "scripts": {
        "docs": "typedoc",
        "build": "pnpm run -w build-package tl-runtime"
    },
    "browser": {
        "./src/encodings/hex.js": "./src/encodings/hex.web.js",
        "./src/encodings/utf8.js": "./src/encodings/utf8.web.js",
        "./src/encodings/base64.js": "./src/encodings/base64.web.js"
    },
    "distOnlyFields": {
        "exports": {
            ".": {
                "import": "./esm/index.js",
                "require": "./cjs/index.js"
            }
        }
    },
    "main": "src/index.ts",
    "dependencies": {
        "long": "5.2.3"
    }
}
